<SCRIPT language=javascript>
	function checkempty(myform)
	{
		if(myform.member_uid.value=="")
		{
			alert("Please inout you member ID!");
			myform.member_uid.focus();
			return false;
		}
		
		if(myform.psd.value == "")
		{
			alert("Please input password!");
			myform.psd.focus();
			return false;
		}
		
		if(myform.re_psd.value == "")
		{
			alert("Please reenter password!");
			myform.re_psd.focus();
			return false;
		}
		
		if(myform.psd.value != myform.re_psd.value)
		{
			alert("Reenter password does not confirm with the one you've setup, please reenter!");
			myform.re_passwd.focus();
			return false;
		}
		
		if(myform.name.value == "")
		{
			alert("Please input your full name");
			myform.name.focus();
			return false;
		}
		
		if(myform.email.value == "")
		{
			alert("Please input your e-mail address!");
			myform.email.focus();
			return false;
		}
		
		if(myform.country[myform.country.selectedIndex].value == "")
		{
			alert("Please select country/region!");
			myform.country.focus();
			return false;
		}
		
		if(myform.city[myform.city.selectedIndex].value == "")
		{
			alert("Please input city!");
			myform.city.focus();
			return false;
		}
		
		if(myform.company.value == "")
		{
			alert("Please input company name!");
			myform.company.focus();
			return false;
		}
		
		if(myform.com_tel.value == "")
		{
			alert("Please input your telphone!");
			myform.com_tel.focus();
			return false;
		}
		
		if(myform.main_products[myform.main_products.selectedIndex].value == "")
		{
			alert("Please input main products/services!");
			myform.main_products.focus();
			return false;
		}
		
		if(myform.biznature[myform.biznature.selectedIndex].value == "")
		{
			alert("Please select business nature!");
			myform.biznature.focus();
			return false;
		}
		
		if(myform.bigclass[myform.bigclass.selectedIndex].value == "")
		{
			alert("Please select major business area!");
			myform.bigclass.focus();
			return false;
		}
		
		if(myform.smallclass[myform.smallclass.selectedIndex].value == "")
		{
			alert("Please select major business area!");
			myform.smallclass.focus();
			return false;
		}
		
	}
